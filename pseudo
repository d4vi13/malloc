block = topoInicialHeap
while(block != topoAtualHeap)
    if(block->used || block->size < requested)
        block = block + block->size + 16 bytes 
        continue
    else
        # retorna 0 se deu, o tam se nao deu
        # se o segundo parametro for menor que zero vai dar erro
        err = createBlock(block + requested + 16 , block->size - requested - 16)
        alocaBlock(block, requested+err)
        break
        
if (block == topoAtualHeap)
    aux = topoAtualHeap
    topoAtualHeap += teto(requested/ 4096) * 4096
    brk(topoAtualHeap)
    alocaBlock(aux, requested)
    alocaMem(requested)
    
    

